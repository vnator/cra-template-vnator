  "scripts": {
    "start": "npx dotenv -e ./.env/local react-scripts start",
    "test": "npx dotenv -e ./.env/local react-scripts test",
    "build": "npx dotenv -e ./.env/local react-scripts build",
    "build:development": "npx dotenv -e ./.env/development react-scripts build",
    "build:staging": "npx dotenv -e ./.env/staging react-scripts build",
    "build:production": "npx dotenv -e ./.env/production react-scripts build",
    "eject": "react-scripts eject",
    "lint:css": "npx stylelint src/**/*.css",
    "lint:ts": "npx eslint -c .eslintrc --ignore-path .eslintignore '**/*.{ts,tsx}' --quiet",
    "lint:ts:fix": "npx eslint -c .eslintrc --ext .ts --ext .tsx --ignore-path .eslintignore --fix '**/*.{ts,tsx}' --quiet"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.3.2",
    "redux-mock-store": "^1.5.4",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-declaration-use-variable": "^1.7.3",
    "stylelint-prettier": "^1.2.0"
  }
